{
    "useTabs": false,
    "tabWidth": 4,
    "arrowParens": "avoid",
    "singleQuote": false,
    "trailingComma": "none",
    "printWidth": 120,
    "bracketSpacing": true,
    "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
    "tailwindConfig": "./tailwind.config.cjs",
    "pluginSearchDirs": ["."],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
